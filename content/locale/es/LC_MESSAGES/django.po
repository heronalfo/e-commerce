#: config/settings/i18n.py:7
msgid "Portuguese"
msgstr "Portugués"

msgid "The username must not be longer than 32 characters"
msgstr "El nombre de usuario no debe tener más de 32 caracteres"

msgid "This username has already been registered"
msgstr "Este nombre de usuario ya ha sido registrado"

msgid "Password must contain at least 8 characters"
msgstr "La contraseña debe contener al menos 8 caracteres"

msgid "The name for username is invalid, it should be like: username-name"
msgstr "El nombre de usuario es inválido, debe ser como: nombre-de-usuario"

msgid "The CPF sent does not comply with Brazilian standards. Example: 123.456.789.01"
msgstr "El CPF enviado no cumple con los estándares brasileños. Ejemplo: 123.456.789.01"

msgid "The CEP sent does not comply with Brazilian standards. Example: 12345-678"
msgstr "El CEP enviado no cumple con los estándares brasileños. Ejemplo: 12345-678"

msgid "The NUMBER sent does not comply with Brazilian standards. Example: +55 (11) 9 1234 5678"
msgstr "El NÚMERO enviado no cumple con los estándares brasileños. Ejemplo: +55 (11) 9 1234 5678"

msgid "The entered category does not exist in the database"
msgstr "La categoría ingresada no existe en la base de datos"

msgid "The price must be greater than 0"
msgstr "El precio debe ser mayor que 0"

msgid "It is not possible to register with a number less than 0"
msgstr "No es posible registrar un número menor que 0"

msgid "The entered product does not exist in the database"
msgstr "El producto ingresado no existe en la base de datos"

msgid "The rating cannot be less than 1 nor greater than 5"
msgstr "La calificación no puede ser inferior a 1 ni superior a 5"